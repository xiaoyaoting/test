{
  "name": "@abtnode/client",
  "version": "1.8.64",
  "description": "graphql client to read/write data on abt node",
  "keywords": [
    "graphql",
    "client",
    "arcblock"
  ],
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    "wangshijun <shijun@arcblock.io>"
  ],
  "author": {
    "name": "wangshijun",
    "email": "shijun@arcblock.io",
    "url": "https://github.com/wangshijun"
  },
  "homepage": "https://github.com/ArcBlock/blocklet-server/core/client",
  "license": "Apache-2.0",
  "main": "./lib/node.js",
  "browser": "./dist/browser.js",
  "files": [
    "docs",
    "dist",
    "lib"
  ],
  "dependencies": {
    "@arcblock/sdk-util": "^0.33.0",
    "@ocap/util": "1.18.37",
    "@ocap/wallet": "1.18.37",
    "axios": "^0.27.2",
    "blueimp-md5": "^2.19.0",
    "core-js": "^3.25.5",
    "debug": "^4.3.4",
    "eventemitter3": "^4.0.7",
    "lodash": "^4.17.21",
    "react-app-polyfill": "^1.0.6"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github",
      [
        "validate-links",
        {
          "repository": "ArcBlock/abtnode"
        }
      ]
    ]
  },
  "scripts": {
    "lint": "eslint src tests",
    "lint:fix": "eslint --fix src tests",
    "clean": "rm -rf lib dist",
    "build-browser": "cross-env BABEL_ENV=browser babel ./src --out-dir ./dist --source-maps --copy-files && webpack --mode production && rm -rf ./dist/node.* ./dist/proto",
    "build-node": "babel ./src --out-dir ./lib --source-maps --copy-files && rm -rf ./lib/browser.* ./lib/proto/*.js.map",
    "build": "npm run clean && npm run build-node && npm run build-browser",
    "build:watch": "babel ./src --out-dir ./lib --source-maps --copy-files --watch",
    "generate-types": "node tools/generate-types.js && sed -i -E \"s/UNSUPPORTED_TX/OK/\" src/types.js",
    "generate-queries": "node tools/generate-queries.js",
    "format-docs": "remark . -o",
    "generate-dts": "node tools/generate-dts.js",
    "update-schema": "rm -f src/schema/graphql.json && node tools/update-graphql-schema.js",
    "upgrade": "npm run update-schema && npm run docs",
    "docs": "npm run build && npm run generate-queries && npm run generate-types && npm run generate-dts && npm run format-docs && npm run build && rm -f src/types.js-E && npm run lint:fix",
    "test": "node tools/jest.js",
    "coverage": "npm run test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArcBlock/blocklet-server.git"
  },
  "bugs": {
    "url": "https://github.com/ArcBlock/blocklet-server/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "graphql": "16.5.0",
    "graphql-request": "^4.3.0",
    "inspectpack": "^4.7.1",
    "jest": "^27.5.1",
    "remark-cli": "^8.0.1",
    "remark-preset-github": "^1.0.1",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0"
  },
  "gitHead": "2abf7d3078ae9251ddeb394b4f0d82de4d9ababe"
}
