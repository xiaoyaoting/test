{
  "name": "@blocklet/sdk",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.8.64",
  "description": "graphql client to read/write data on abt node",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "service"
  ],
  "scripts": {
    "verify": "npm run lint:fix && npm run test && npm run build",
    "lint": "eslint src tests service",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --forceExit --detectOpenHandles",
    "coverage": "npm run test -- --coverage",
    "clean": "rm -fr lib",
    "prebuild": "npm run clean && npm run types",
    "types": "rm -rf ./src/types && node tools/version.js && ts-node tools/types.js && rm -f ./src/types/index.ts && prettier --write ./src/types/notification.ts",
    "build": "tsc",
    "build:watch": "npm run build -- -w"
  },
  "author": "linchen1987 <linchen.1987@foxmail.com> (http://github.com/linchen1987)",
  "license": "MIT",
  "dependencies": {
    "@abtnode/client": "1.8.64",
    "@abtnode/constant": "1.8.64",
    "@arcblock/did-auth": "1.18.37",
    "@arcblock/jwt": "1.18.37",
    "@arcblock/ws": "1.18.37",
    "@blocklet/constant": "1.8.64",
    "@blocklet/meta": "1.8.64",
    "@did-connect/authenticator": "^2.1.33",
    "@did-connect/handler": "^2.1.33",
    "@nedb/core": "^2.1.3",
    "@ocap/mcrypto": "1.18.37",
    "@ocap/wallet": "1.18.37",
    "axios": "^0.27.2",
    "fs-extra": "^10.1.0",
    "joi": "17.7.0",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "url-join": "^4.0.1"
  },
  "resolutions": {
    "@nedb/core": "^2.1.2"
  },
  "devDependencies": {
    "@arcblock/eslint-config-ts": "^0.2.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/json-stable-stringify": "^1.0.34",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "detect-port": "^1.5.1",
    "eslint": "^8.25.0",
    "jest": "^29.2.0",
    "joi-to-typescript": "^4.0.6",
    "json-stable-stringify": "^1.0.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "gitHead": "2abf7d3078ae9251ddeb394b4f0d82de4d9ababe"
