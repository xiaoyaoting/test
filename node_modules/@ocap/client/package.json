{
  "name": "@ocap/client",
  "version": "1.18.43",
  "description": "graphql client to read/write data on arcblock blockchain",
  "keywords": [
    "graphql",
    "client",
    "blockchain",
    "arcblock"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "wangshijun",
    "email": "shijun@arcblock.io",
    "url": "https://github.com/wangshijun"
  },
  "contributors": [
    "wangshijun <shijun@arcblock.io> (https://github.com/wangshijun)"
  ],
  "homepage": "https://github.com/arcblock/blockchain",
  "license": "Apache-2.0",
  "main": "./lib/node.js",
  "browser": "./dist/browser.js",
  "files": [
    "docs",
    "examples",
    "dist",
    "lib"
  ],
  "dependencies": {
    "@arcblock/did": "1.18.43",
    "@arcblock/did-util": "1.18.43",
    "@arcblock/jwt": "1.18.43",
    "@arcblock/sdk-util": "^0.33.0",
    "@arcblock/ws": "1.18.43",
    "@ocap/asset": "1.18.43",
    "@ocap/mcrypto": "1.18.43",
    "@ocap/message": "1.18.43",
    "@ocap/proto": "1.18.43",
    "@ocap/util": "1.18.43",
    "@ocap/wallet": "1.18.43",
    "blueimp-md5": "^2.19.0",
    "buffer": "6.0.3",
    "debug": "^4.3.4",
    "is-absolute-url": "^3.0.3",
    "jsonpack": "^1.1.5",
    "lodash": "^4.17.21",
    "react-app-polyfill": "^1.0.6",
    "readable-stream": "3.6.0",
    "wolfy87-eventemitter": "^5.2.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^8.25.0",
    "graphql": "16.5.0",
    "graphql-request": "^4.3.0",
    "inspectpack": "^4.7.1",
    "jest": "^27.5.1",
    "jsdoc-to-markdown": "^7.1.1",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "prettier": "^2.7.1",
    "remark-cli": "^10.0.1",
    "remark-preset-github": "^4.0.4",
    "rimraf": "^2.7.1",
    "shelljs": "^0.8.5",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^4.10.0"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github",
      [
        {
          "repository": "ArcBlock/asset-chain"
        }
      ]
    ]
  },
  "scripts": {
    "lint": "eslint src examples tests",
    "lint:fix": "eslint --fix src examples tests",
    "clean": "rimraf lib && rimraf dist",
    "build-browser": "BABEL_ENV=browser babel ./src --out-dir ./dist --source-maps --copy-files && webpack && rm -f ./dist/node.*",
    "build-node": "babel ./src --out-dir ./lib --source-maps --copy-files && rm -f ./lib/browser.*",
    "build": "npm run clean && npm run build-node && npm run build-browser",
    "build:watch": "babel ./src --out-dir ./lib --source-maps --copy-files --watch",
    "gen-docs": "jsdoc2md src/client.js src/methods.js src/types.js > docs/README.md",
    "gen-types": "node tools/gen-types.js && sed -i -E \"s/UNSUPPORTED_TX/OK/\" src/types.js && rm -f src/types.js-E",
    "gen-queries": "node tools/gen-queries.js",
    "format-docs": "remark . -o",
    "examples": "node tools/run-example.js",
    "gen-dts": "node tools/gen-dts.js && prettier --write src/*.d.ts",
    "update-schema": "rm -f src/schema/graphql.json && node tools/update-graphql-schema.js",
    "upgrade": "npm run update-schema && npm run build && npm run gen-queries && npm run gen-types && npm run docs && npm run build && npm run lint:fix",
    "docs": "npm run gen-dts && npm run gen-docs && npm run cleanup-docs && npm run format-docs",
    "cleanup-docs": "node ../../scripts/cleanup-docs.js docs/README.md $npm_package_name",
    "test": "jest --forceExit --detectOpenHandles",
    "coverage": "npm run test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArcBlock/asset-chain.git"
  },
  "bugs": {
    "url": "https://github.com/ArcBlock/asset-chain/issues"
  },
  "gitHead": "8eb58bc4a266b5ff60ad70f6ff88b15e45474abd"
}
